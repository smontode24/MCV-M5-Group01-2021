#!/bin/bash
#SBATCH -n 4 # Number of cores
#SBATCH -J eval_faster_random #job_name 
#SBATCH --mem 14000 # 2GB solicitados.
#SBATCH -p mhigh,mlow # or mlow Partition to submit to master low prioriy queue
#SBATCH --gres gpu:1 # Para pedir Pascales MAX 8
#SBATCH -o %x_%u_%j.out # File to which STDOUT will be written
#SBATCH -e %x_%u_%j.err # File to which STDERR will be written


for f in $(ls ~/W3_random_search/); do
  /home/group01/anaconda3/envs/pytorch/bin/python ../kitti_project/test_from_random_search.py --experiment_name $f --model_name resnet50_fpn --batch_size 2 --balanced_weights --focal_loss
done
